{
    "name": "Admin Settings Preview",
    "description": "Preview admin settings config changes.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "activeTab",
      "webRequest",
      "webRequestBlocking",
      "storage",
      "*://*.myfreshworks.dev//"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
      },
      "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
      }
    }
  }